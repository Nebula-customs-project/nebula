# =============================================================================
# Nebula Docker Environment Configuration - Example
# Copy this file to .env and update with your actual values
# =============================================================================

# PostgreSQL Configuration
POSTGRES_DB=nebula_db
POSTGRES_USER=nebula_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_PORT=5434

# RabbitMQ Configuration
RABBITMQ_USER=nebula
RABBITMQ_PASSWORD=your_secure_password_here
RABBITMQ_VHOST=/
RABBITMQ_AMQP_PORT=5672
RABBITMQ_MQTT_PORT=1883
RABBITMQ_MANAGEMENT_PORT=15672
RABBITMQ_WEB_MQTT_PORT=15675